{
  "name": "@vimlet/commons",
  "version": "2.5.4",
  "description": "Provide utilities for common tasks such as, file compression, file download, OS integration, exec, etc",
  "main": "index.js",
  "dependencies": {
    "fs-extra": "^4.0.1",
    "request": "^2.81.0",
    "multipipe": "^1.0.2",
    "compressing": "1.2.3",
    "glob":"^7.1.2"
  },
  "devDependencies": {
    "@vimlet/alpha": "0.0.18"
  },
  "author": "Vimlet",
  "repository": {
    "type": "git",
    "url": "https://github.com/vimlet/VimletCommons.git"
  },
  "license": "BSD-2-Clause-FreeBSD",
  "keywords": [
    "zip",
    "tar",
    "tgz",
    "compress",
    "uncompress",
    "spawn",
    "exec",
    "run",
    "download",
    "os"
  ],
  "scripts": {
    "format:js": "alpha format-js",
    "format:html": "alpha format-html",
    "format:css": "alpha format-css",
    "format": "run-s format:js format:html format:css",
    "test": "alpha test",
    "doc:api": "alpha doc api -i **/*.js,**/*.html -o ../../docs/node/api",
    "doc:article": "alpha doc article -i ../docs/md/**/*.md -o ../../docs/node/article",
    "doc": "run-s doc:api doc:article",
    "badges": "alpha badges -o ../../badges"
  }
}
