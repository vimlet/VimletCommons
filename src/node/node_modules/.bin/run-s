#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@vimlet/alpha/node_modules/npm-run-all/bin/run-s/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../@vimlet/alpha/node_modules/npm-run-all/bin/run-s/index.js" "$@"
  ret=$?
fi
exit $ret
