{
  "name": "commons",
  "version": "0.0.1",
  "description": "Provide utilities for common tasks such as, file compression, file download, OS integration, exec, etc",
  "main": "",
  "optionalDependencies": {
    "@vimlet/alpha": "0.4.34"
  },
  "author": "Vimlet",
  "license": "MIT",
  "keywords": [
    "zip",
    "tar",
    "tgz",
    "compress",
    "uncompress",
    "spawn",
    "exec",
    "run",
    "download",
    "os"
  ],
  "scripts": {
    "format:js": "alpha format-js",
    "format:html": "alpha format-html",
    "format:css": "alpha format-css",
    "format": "run-s format:js format:html format:css",
    "test": "alpha test",
    "doc:api": "alpha doc api -i **/*.js,**/*.html -o ../../docs/node/api",
    "doc:article": "alpha doc article -i ../../docs/md/**/*.md -o ../../docs/node/article",
    "doc:main": "alpha doc main -p ../../docs/node/api -r ../../docs/node/article -o ../../docs",
    "doc": "run-s doc:api doc:article doc:main",
    "readme": "alpha readme",
    "meta": "alpha meta -i **/*.vmt"
  }
}
